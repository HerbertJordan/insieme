libPaths="./"
libNames="icl","m"
postprocessing=
executionFlags=-check
use_c[insiemecc_ocl_c_compile]=1

definitions[insiemecc_ocl_c_compile]="_GNU_SOURCE","USE_OPENCL=ON","LOCAL_MODE"
outputAwk="/Result check: OK/"
