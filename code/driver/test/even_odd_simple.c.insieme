

//typedef int bool;
#define bool int

#define true 1
#define false 0

extern int printf(char *, ...);

bool even(unsigned x);
bool odd(unsigned x);

bool even(unsigned x) {
	if (x==0) return true;
	return odd(x-1);
}

bool odd(unsigned x) {
	if (x==0) return false;
	return even(x-1);
}

char* toStr(bool value) {
	if (value) return "true";
	return "false";
}


